User
    ident Text
    password Text Maybe
    UniqueUser ident
    numEntries Int
    avgVote Int
    numComments Int
    numVotes Int
    name Text
    image Text Maybe
    signupTime UTCTime
    lastSeen UTCTime
    deriving Typeable
Comment
    message Text
    userId UserId
--    entryId EntryId
    timeStamp UTCTime
    deriving Eq
    deriving Show
Entry
    userId UserId
    isImage Bool
    avgVote Int
    numVotes Int
    numComments Int
    timeStamp UTCTime
    title Text
    url Text
    UniqueEntry url
    deriving Eq
    deriving Show
EntryComment
    entry EntryId
    comment CommentId
EntryVote
    entry EntryId
    vote VoteId
Vote
    value Int
--    entryId EntryId
    userId UserId
    timeStamp UTCTime
    deriving Eq
    deriving Show

